/**servidor_ativo_3**/
.box-itens-escritorio section#tipos_clientes p {
	text-align: left;
}

.box-itens-escritorio section#tipos_clientes ul.tipos_clientes_3_menu li {
	width: auto;
	border-right: none;
	min-height: inherit;
	height: auto;
	padding: 1.0rem 0.0rem;
	text-align: center;

	margin-right: -1px;
	float: none;
	clear: both;
	margin: 0 0px 0 0;
	border-bottom: 1px dashed #CCC;
	background: #FBFBFB;
}
.box-itens-escritorio section#tipos_clientes ul.tipos_clientes_3_menu li:last-child {

}
.box-itens-escritorio section#tipos_clientes ul.tipos_clientes_3_menu {
	border-bottom: 0px;
	width: 7%;
	margin: 0;
	float: left;
}

.box-itens-escritorio section#tipos_clientes ul.tipos_clientes_3_menu li.active {
	opacity: 1;
	border-right: 1px solid white;
	background: white;
	position: relative;
	z-index: 3;
	box-shadow: -3px 2px 7px -2px #AAA;
}

.box-itens-escritorio section#tipos_clientes ul.tipos_clientes_3_menu li img.image_on {
		display: none;
}
.box-itens-escritorio section#tipos_clientes ul.tipos_clientes_3_menu li img.image_off {
		display: block;
}
.box-itens-escritorio section#tipos_clientes ul.segmentos_menu li:hover img.image_on,
.box-itens-escritorio section#tipos_clientes ul.tipos_clientes_3_menu li.active img.image_on {
		display: block;
}
.box-itens-escritorio section#tipos_clientes ul.segmentos_menu li:hover img.image_off,
.box-itens-escritorio section#tipos_clientes ul.tipos_clientes_3_menu li.active img.image_off {
		display: none;
}
.box-itens-escritorio section#tipos_clientes div.tipos_clientes_3_texto {
	padding-top: 40px;
	min-height: 300px;
	float: left;
	box-shadow: -3px 2px 7px -2px #AAA;
	margin-left: -1px;
	z-index: 1;
	position: relative;
	padding: 30px;
	width: 720px;
}

.box-itens-escritorio section#exemplosCampoInteresse ul li:first-child {
	padding-top: 0px;
}


input[type="submit"].button-busy {
    background: #444;
    cursor: default;
}
input[type="submit"].button-success {
    background: green;
	-webkit-box-shadow: none;
    -moz-box-shadow: none;
    box-shadow: none;
    top: 0;
    left: 0;
    cursor: default;
}
input[type="submit"].button-error {
    background: #F7C5C5;
    color: red;
}

#mensagem_erro p{
	margin: 7px;
	font-size: 1.2rem;
}

#mensagem_erro strong {
	color: red;
}
#contato_form {
	display: none;
	position: absolute;
	top: 575px;
	left: 0;
	width: 100%;
	background: rgb(236, 236, 236);
	text-align: center;
	padding: 20px;
	box-shadow: 1px 0px 10px 0px;
}
#contato_form input[type="text"],
#contato_form select{
	font-size: 1.4rem;
	margin-bottom: 0rem;
	padding: 10px;
	width: 50%;
}
