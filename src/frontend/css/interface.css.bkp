/*Advogados home*/

.destaque-advogados{
	clear: both;
	background-color: #cfc9cd;
	height: 160px;
	position: relative;
	margin-bottom: 60px;
}
.destaque-advogados ul {
    width: 20000em;
    position: relative;

    /* Optional, required in this case since it's a <ul> element */
    list-style: none;
    margin: 0;
    padding: 0;
}
.destaque-advogados li{
	float: left;
	width: 170px;
	height: 160px;
}
.destaque-advogados li img{
	filter: gray; /* IE6-9 */
	-webkit-filter: grayscale(1); /* Webkit Nightlies & Google Chrome Canary */
	/* css transitions */
	-webkit-transition: all .4s ease;  /* Safari 3.2+, Chrome */
	-moz-transition: all .4s ease;  /* Firefox 4-15 */
	-o-transition: all .4s ease;  /* Opera 10.5–12.00 */
	transition: all .4s ease;
}
.destaque-advogados li a:hover img{
	filter: none; /* Applies to FF + IE */
	-webkit-filter: grayscale(0);
	/* css transitions */
	-webkit-transition: all .3s ease;  /* Safari 3.2+, Chrome */
	-moz-transition: all .3s ease;  /* Firefox 4-15 */
	-o-transition: all .3s ease;  /* Opera 10.5–12.00 */
	transition: all .3s ease;  /* Firefox 16+, Opera 12.50+ */
}
.destaque-advogados .previous
, .destaque-advogados .next{
	position: absolute;
	top: 55px;
	z-index: 99;
	border: none;
	overflow: hidden;
	text-indent: -999em;
	width: 30px;
	height: 50px;
	cursor: pointer;
}
.destaque-advogados .previous{
	background: url(../img/ui/previous-advogados.png) no-repeat;
	left: -20px;
}
.destaque-advogados .next{
	background: url(../img/ui/next-advogados.png) no-repeat;
	right: -20px;
}

