@import "reset.css";
@import "global.css";
@import "font/style.css";



html{

}
a{
	text-decoration:none;
}
h1,h2,h3,h4,h5{
	font-weight: normal;
}
a:hover{
	text-decoration:underline;
}
body{
	font-size: 14px;
	color: #111111;
	font-family: 'Avenir Next LT Pro Regular';
}

#main-top .center
, #main-bottom .center{
	margin:0 auto;
	width: 1000px;
}
 #main-top{
 	background-color: #fafbfa;
 	padding-bottom: 90px;
 	float: left;
 	width: 100%;
 }

.mobile_only {
	display: none;
}
.desktop_only {
	display: block;
}

/*HEADER*/

header{
	width: 100%;
	height: 145px;
}
header h1{
	float: left;
	margin: 25px 0 17px 0;
}
header h1 a{
	background: url(../img/tit/logo.png) no-repeat;
	width: 182px;
	height: 60px;
	overflow: hidden;
	text-indent: -999em;
	display: block;
}
header #nav{
	width: 880px;
	padding: 0 60px;
}
header #nav nav{
	float: right;
	margin-top: 30px;
}

header .nav-blog{
	background-color: #d60031;
	padding: 8px 60px;
	width: 880px;
	height: 24px;
	-webkit-border-bottom-right-radius: 5px;
	-webkit-border-bottom-left-radius: 5px;
	-moz-border-radius-bottomright: 5px;
	-moz-border-radius-bottomleft: 5px;
	border-bottom-right-radius: 5px;
	border-bottom-left-radius: 5px;
}
header .nav-blog h2{
	float: left;
	text-transform: uppercase;
	font-weight: bold;
	font-family: 'Avenir Next LT Pro Ultra Light Condensed';
	color: #ffffff;
	font-size: 11px;
	margin-top: 8px;
}
header .nav-blog h2 span{
	font-family: 'Avenir Next LT Pro Ultra Light Condensed';
	font-weight: normal;
}
header .nav-blog ul{
	float: right;
}
header .nav-blog li{
	float: left;
	padding: 7px 29px
}
header .nav-blog li:last-child{
	background: url(../img/bg/border.png) repeat-y left;
	padding-right: 0;
}
header .nav-blog li:first-child{
	background: url(../img/bg/border.png) repeat-y right;
}
header .nav-blog li a{
	font-style: 11px;
	color: #ffffff;
}
header nav ul li{
	float: left;
	margin: 10px;
}
header nav ul li:last-child{
	margin-right: 0;
}
header nav ul li a{
	color: #555555;
	text-transform: uppercase;
	font-size: 12px;
	border: 3px solid #fafbfa;
	padding: 14px;
}
header nav ul li a:hover
, header nav ul li a.ativo{
	color: #d60031;
	text-decoration: none;
	font-family: 'Avenir Next LT Pro Demi';
	border: 3px solid #d60031;
}

/*CONTENT*/

.content{
	clear: both;
}

/*HOME*/

/*Destaque home*/

#destaque aside{
 	float: left;
 	width: 300px;
 	padding: 0 15px;
 	background-color: #d60031;
 }
 #destaque aside li{
  	padding: 25px 0;
