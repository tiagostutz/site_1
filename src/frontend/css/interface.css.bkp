}
#destaque section{
  	float: left;
  	width: 670px;
}
#destaque section .slider{
	height: 360px;
}
#destaque section .slider li{
	float: left;
	height: 360px;
}
#destaque section .next
, #destaque section .previous{
	position: relative;
	z-index: 1000;
	background: transparent;
	border: none;
	font-size: 2.145rem;
	color: rgba(230,230,230,.5);
	text-shadow: 1px 1px 1px rgba(30,30,30,.3);
	font-family: verdana;
	cursor: pointer;
	margin-top: 140px;
}
#destaque section .previous {
	margin-right: -45px;
	float: left;
}
#destaque section .next {
	float: right;
	margin-left: -45px;
}

#destaque a,
#destaque a:link,
#destaque a:visited,
#destaque a:hover
#destaque a p {
	text-decoration: none;
}
#destaque p.text_holder {
	position: relative;
	top: -365px;
	background: white;
	padding: 10px;
	text-transform: uppercase;
	font-size: 1.575rem;
	height: 153px;
	font-weight: normal;
	width: 650px;
	text-decoration: none;
	color: white;
	line-height: 2.6rem;
	background: rgba(0,0,0,.67);
	text-align: center;
	display: block;
	margin: auto;
	text-shadow: black 1px 3px 1px;
	padding-top: 210px;
	letter-spacing: -1px;
	font-family: 'Avenir Next LT Pro Demi',arial;
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(0,0,0,.1)), color-stop(69%,rgba(0,0,0,0.73)));
	background: rgba(0,0,0,.2); /* Old browsers */
	/* IE9 SVG, needs conditional override of 'filter' to 'none' */
	background: url(data:image/svg+xml;base64,);
	background: -moz-linear-gradient(top,  rgba(0,0,0,.1) 0%, rgba(0,0,0,0.73) 69%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(0,0,0,.1)), color-stop(69%,rgba(0,0,0,0.73))); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  rgba(0,0,0,.1) 0%,rgba(0,0,0,0.73) 69%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  rgba(0,0,0,.1) 0%,rgba(0,0,0,0.73) 69%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  rgba(0,0,0,.1) 0%,rgba(0,0,0,0.73) 69%); /* IE10+ */
	background: linear-gradient(to bottom,  rgba(0,0,0,.1) 0%,rgba(0,0,0,0.73) 69%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='rgba(0,0,0,.1)', endColorstr='rgba(0,0,0,0.73)',GradientType=0 ); /* IE6-8 */
}

